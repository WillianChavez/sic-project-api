{
  "name": "api-sic",
  "version": "1.0.0",
  "description": "",
  "main": "./app.js",
  "directories": {
    "test": "tests"
  },
  "engineStrict": true,
  "engines": {
    "npm": "6.14.18",
    "node": "14.21.3"
  },
  "scripts": {
    "start": "nodemon app.mjs",
    "start:test": "export APP_ENV=test&& nodemon app.mjs",
    "serve": "webpack --config webpack.config.js",
    "build": "webpack --config webpack.config.build.js",
    "test": "mocha --u bdd tests/**/*.spec.mjs",
    "tdd": "mocha --u bdd",
    "swagger": "yamlinc -o ./app/docs/index.yaml ./app/docs/swagger/include.yaml && yaml2json ./app/docs/test.yaml > ./app/docs/test.json",
    "migration:generate": "sequelize-cli migration:generate --name",
    "migrate": "sequelize-cli db:migrate",
    "migrate:test": "export APP_ENV=test&& sequelize-cli db:migrate",
    "migration:revert": "sequelize-cli db:migrate:undo",
    "migration:delete": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:test": "export APP_ENV=test&& sequelize-cli db:seed:all",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
